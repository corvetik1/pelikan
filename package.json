{
  "name": "bahtapelikanoff",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --passWithNoTests",
    "e2e": "playwright test",
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "migrate": "prisma migrate deploy",
    "seed": "prisma db seed",
    "db:up": "docker compose up -d db",
    "db:down": "docker compose down",
    "db:reset": "docker compose down -v && docker compose up -d db",
    "db:setup": "wait-on tcp:5434 && pnpm migrate && pnpm seed",
    "monitor:up": "docker compose -f docker-compose.yml -f docker-compose.monitoring.yml up -d",
    "monitor:down": "docker compose -f docker-compose.yml -f docker-compose.monitoring.yml down",
    "docs:openapi": "tsx scripts/generate-openapi.ts",
    "docs:build": "redoc-cli bundle docs/openapi.json -o public/api.html",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@emotion/server": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@mui/x-data-grid": "^7.1.0",
    "@prisma/client": "^6.11.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/xlsx": "^0.0.36",
    "@uiw/react-markdown-preview": "^5.1.4",
    "@uiw/react-md-editor": "^4.0.8",
    "dayjs": "^1.11.10",
    "framer-motion": "^12.23.6",
    "leaflet": "^1.9.4",
    "next": "15.3.4",
    "nodemailer": "^6.9.8",
    "pino": "^9.7.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^4.2.1",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "recharts": "^2.8.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8",
    "react-dropzone": "^14.2.3",
    "jszip": "^3.10.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@mswjs/data": "^0.16.2",
    "@playwright/test": "^1.44.0",
    "@storybook/addon-essentials": "^8.0.0-rc.0",
    "@storybook/nextjs": "^8.0.0-rc.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.8",
    "@types/leaflet": "^1.9.6",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "cross-fetch": "^3.1.9",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.1",
    "eslint-plugin-prettier": "^5.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "msw": "^1.3.5",
    "prettier": "^3.2.5",
    "prisma": "^6.11.0",
    "redoc-cli": "0.13.21",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^3.13.0",
    "typescript": "^5",
    "@playwright/test": "^1.44.1",
    "wait-on": "^7.1.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "msw": {
    "workerDirectory": "public"
  }
}